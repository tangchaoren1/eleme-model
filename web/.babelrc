{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-3"
  ],
  "plugins": [
      "transform-async-to-generator",
      "transform-es2015-modules-commonjs",
      "transform-export-extensions",
      "transform-vue-jsx", "transform-runtime"
  ]
}
